{
    "compilerOptions":
    {
        "target": "es6",
        "module": "commonjs",
        "declaration": true,
        "noImplicitAny": false,
        "moduleResolution": "node",
        "removeComments": true,
        "sourceMap": true,
        "outDir": "dist/",
        "types": ["google-apps-script"]
    },
    "include": [
        "ImportJSON.gs",
        "FetchData.gs"
    ],
    "exclude": [
        "node_modules"
    ]
}